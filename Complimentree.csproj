<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="wwwroot\images\red_trees.jpg" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\images\green_trees.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="wwwroot\images\red_trees.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
